#include <iostream>
using namespace std;
int main() {
    int count =0;
    int arr[2][2][3];
    
    for(int i=0;i<2;i++){
        for(int j=0;j<2;j++){
            for(int k=0;k<3;k++){
            
            arr[i][j][k] = count;
            count++;
        }
        }
    }
     for(int i=0;i<2;i++){
        for(int j=0;j<2;j++){
            for(int k=0;k<3;k++){
            cout<<"arr[i][j][k] ="<<arr[i][j][k]<<endl;
            arr[2][2][3];
            count++;
        }
        }
     }
    return 0;
}
